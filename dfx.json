{
  "canisters": {
    "markets": {
      "dependencies": [
        "ledger"
      ],
      "main": "markets-canister/main.mo",
      "type": "motoko"
    },
    "ledger": {
      "main": "markets-canister/Ledger.mo",
      "type": "motoko"
    },
    "assets": {
      "dependencies": [
        "markets"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": [
        "dist/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
